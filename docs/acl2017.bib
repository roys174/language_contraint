@article{Newman:2003,
  title={Lying words: Predicting deception from linguistic styles},
  author={Newman, Matthew L. and Pennebaker, James W. and Berry, Diane S. and Richards, Jane M.},
  journal={Personality and Social Psychology Bulletin},
  volume={29},
  number={5},
  pages={665--675},
  year={2003},
 % publisher={Sage Publications},
  abstract={Telling lies often requires creating a story about an experience or
attitude that does not exist. As a result, false stories may be qualitatively
different from true stories. The current project investigated
the features of linguistic style that distinguish between true
and false stories. In an analysis of five independent samples, a
computer-based text analysis program correctly classified liars
and truth-tellers at a rate of 67\% when the topic was constant
and a rate of 61\% overall. Compared to truth-tellers, liars
showed lower cognitive complexity, used fewer self-references and
other-references, and used more negative emotion words.},
}



